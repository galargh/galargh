<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width,minimum-scale=1" name="viewport">

  <title>{{ title }}</title>

  <link rel="stylesheet" href="/styles/tailwind.css">
  <style>
    :root {
      --indigo-600: rgba(42, 40, 56, 1);
      --indigo-500: rgba(42, 40, 56, 0.9);
      --indigo-400: rgba(42, 40, 56, 0.8);
      --indigo-300: rgba(42, 40, 56, 0.7);
      --indigo-200: rgba(42, 40, 56, 0.6);
      --indigo-100: rgba(42, 40, 56, 0.5);
      --yellow-600: rgba(253, 230, 175, 1);
      --yellow-50: rgba(253, 230, 175, 0.1);
      --violet-600: rgba(169, 160, 224, 1);
      --violet-50: rgba(169, 160, 224, 0.1);
    }
  </style>

  <link rel="shortcut icon" href="/images/dx.png">
  <link rel="icon" href="/images/dx.png" type="image/png">

  <meta name="description" content="{{ hero_heading }}">
  <link rel="canonical" href="https://galargh.com">

  <meta property="og:site_name" content="DX by galargh">
  <meta property="og:type" content="profile">
  <meta property="og:title" content="DX by galargh">
  <meta property="og:description" content="{{ hero_heading }}">
  <meta property="og:url" content="https://galargh.com">

  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="DX by galargh">
  <meta name="twitter:description" content="{{ hero_heading }}">
  <meta name="twitter:url" content="https://galargh.com">
  <meta name="twitter:site" content="@galargh">
  <meta name="twitter:creator" content="@galargh">

</head>

<body>
  <div>
    <div>
      <header class="bg-(--indigo-600) text-white py-16 md:py-24">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="md:flex-row items-center justify-between flex flex-col">
            <div class="md:w-1/2">
              <img alt="The DX by galargh logo - the letters D and X subtitled with by galargh"
                src="/images/dx.svg" class="max-w-full
              rounded-lg h-auto" />
            </div>
            <div class="md:w-1/2 md:mb-0 mb-10 text-center">
              <p class="text-2xl md:text-3xl font-medium mb-6 animate-bounce">{{ hero_heading }}</p>
              <p class="text-lg mb-8">{{ hero_subheading }}</p>
              <a href="#about" class="inline-flex border border-(--yellow-600) focus:outline-none focus:ring-2 focus:ring-(--yellow-600)
              focus:ring-offset-2 focus:ring-offset-(--indigo-600) hover:border-(--violet-600) transition duration-300 justify-center
              rounded-md py-3 px-6 bg-(--yellow-600) text-(--indigo-600) text-base font-medium shadow-sm m-auto">{{ hero_button_text }}</a>
            </div>
          </div>
        </div>
      </header>
      <section id="about" class="py-16 md:py-24 bg-(--yellow-50)">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="text-center mb-16">
            <p class="text-3xl md:text-4xl font-bold mb-4">{{ about_heading }}</p>
            <div class="w-20 h-1 bg-(--indigo-500) mx-auto mb-6"></div>
            <p class="text-lg text-(--indigo-500) mx-auto max-w-3xl">{{ about_text }}</p>
          </div>
          <div class="md:grid-cols-2 grid grid-cols-1 gap-8">
            {% for card in about_cards %}
            <div class="bg-(--yellow-600) rounded-lg shadow-md p-8 transition duration-300 hover:shadow-xl">
              <div class="text-(--indigo-600) mb-4">
                {{ card.icon | safe }}
              </div>
              <p class="text-xl font-semibold mb-3">{{ card.heading }}</p>
              <p class="text-(--indigo-500)">{{ card.text }}</p>
            </div>
            {% endfor %}
          </div>
        </div>
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container pt-16 md:pt-24">
          <div class="md:flex-row items-center justify-between flex flex-col">
            <div class="md:w-1/2 md:mb-0 mb-10 text-center">
              <p class="text-2xl md:text-3xl font-medium mb-6">{{ about_team_heading }}</p>
              <p class="text-lg mb-8">{{ about_team_text }}</p>
            </div>
            <div class="md:w-1/2 p-16">
              <a href="{{ about_team_photo.href }}">
                <img alt="{{ about_team_photo.alt }}" src="{{ about_team_photo.src }}" class="rounded-full shadow-md transition duration-300 hover:shadow-xl opacity-90" />
              </a>
            </div>
          </div>
        </div>
      </section>
      <section id="services" class="py-16 md:py-24 bg-(--violet-50)">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="text-center mb-16">
            <p class="text-3xl md:text-4xl font-bold mb-4">{{ services_heading }}</p>
            <div class="w-20 h-1 bg-(--indigo-500) mx-auto mb-6"></div>
            <p class="text-lg text-(--indigo-500) mx-auto max-w-3xl">{{ services_text }}</p>
          </div>
          <div class="md:grid-cols-2 lg:grid-cols-3 grid grid-cols-1 gap-8">
            {% for card in services_cards %}
            <div class="bg-(--violet-600) rounded-lg shadow-md p-8 transition duration-300 hover:shadow-xl">
              <div class="text-(--indigo-600) mb-4">
                {{ card.icon | safe }}
              </div>
              <p class="text-xl font-semibold mb-3">{{ card.heading }}</p>
              <p class="text-(--indigo-500)">{{ card.text }}</p>
            </div>
            {% endfor %}
          </div>
        </div>
      </section>
      <section id="products" class="py-16 md:py-24 bg-(--yellow-50)">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="text-center mb-16">
            <p class="text-3xl md:text-4xl font-bold mb-4">{{ products_heading }}</p>
            <div class="w-20 h-1 bg-(--indigo-500) mx-auto mb-6"></div>
            <p class="text-lg text-(--indigo-500) mx-auto max-w-3xl">{{ products_text }}</p>
          </div>
          <div class="md:grid-cols-2 lg:grid-cols-2 grid grid-cols-1 gap-8">
            {% for card in products_cards %}
            <div class="bg-(--yellow-600) rounded-lg shadow-md p-8 transition duration-300 hover:shadow-xl">
              <div class="text-(--indigo-600) mb-4">
                {{ card.icon | safe }}
              </div>
              <p class="text-xl font-semibold mb-3">{{ card.heading }}</p>
              <p class="text-(--indigo-500)">{{ card.text }}</p>
            </div>
            {% endfor %}
          </div>
        </div>
      </section>
      <section id="testimonials" class="py-16 md:py-24 bg-(--violet-50)">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="text-center mb-16">
            <p class="text-3xl md:text-4xl font-bold mb-4">{{ testimonials_heading }}</p>
            <div class="w-20 h-1 bg-(--indigo-500) mx-auto mb-6"></div>
            <p class="text-lg text-(--indigo-500) mx-auto max-w-3xl">{{ testimonials_text }}</p>
          </div>
          <div class="md:grid-cols-3 grid grid-cols-1 gap-8">
            {% for card in testimonials_cards %}
            <div class="bg-(--violet-600) rounded-lg shadow-md p-8 transition duration-300 hover:shadow-xl">
              <div class="items-center mb-6 flex">
                <div class="text-(--indigo-500)">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8" fill="currentColor" viewBox="0 0 24 24">
                    <path
                      d="M14.017 18L14.017 10.609C14.017 4.905 17.748 1.039 23 0L23.995 2.151C21.563 3.068 20 5.789 20 8H24V18H14.017ZM0 18V10.609C0 4.905 3.748 1.039 9 0L9.996 2.151C7.563 3.068 6 5.789 6 8H9.983L9.983 18L0 18Z">
                    </path>
                  </svg>
                </div>
              </div>
              <p class="text-(--indigo-500) mb-6 italic">{{ card.quote }}</p>
              <!--
              <div class="items-center flex">
                <img alt="Portrait of {{ card.name }}, ${{ card.title }}" src="https://placehold.co/60x60/2a2838/fde6af?text={{ card.initials }}"
                  class="h-12 w-12 rounded-full mr-4" />
                <div>
                  <p class="font-semibold">{{ card.name }}</p>
                  <p class="text-(--indigo-400) text-sm">{{ card.title }}</p>
                </div>
              </div>
              -->
            </div>
            {% endfor %}
          </div>
        </div>
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container pt-16 md:pt-24">
          <div class="md:grid-cols-5 grid grid-cols-2 gap-8">
            {% for photo in testimonials_photos %}
            <div class="items-center flex">
              <a href="{{ photo.href }}" class="mx-auto w-full">
                <img alt="{{ photo.alt }}" src="{{ photo.src }}" class="w-full aspect-3/1" />
              </a>
            </div>
            {% endfor %}
          </div>
        </div>
      </section>
      <footer class="bg-(--indigo-600) text-white">
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container py-16 md:py-24">
          <div class="mx-auto text-center max-w-4xl">
            <p class="text-3xl md:text-4xl font-bold mb-6">{{ footer_cta }}</p>
            <div class="sm:flex-row sm:space-y-0 sm:space-x-4 justify-center flex flex-col space-y-4">
              <a href="{{ calendly_link }}" target="_blank" class="inline-flex border border-(--yellow-600) focus:outline-none focus:ring-2 focus:ring-(--yellow-600)
              focus:ring-offset-2 focus:ring-offset-(--indigo-600) hover:border-(--violet-600) transition duration-300 justify-center
              rounded-md py-3 px-6 bg-(--yellow-600) text-(--indigo-600) text-base font-medium shadow-sm m-auto">{{ footer_button_text }}</a>
            </div>
          </div>
        </div>
        <div class="mx-auto px-4 sm:px-6 lg:px-8 container">
          <div class="md:grid-cols-4 grid grid-cols-1 gap-8">
            <div class="md:flex-row items-center justify-between flex flex-col">
              <div class="w-1/2 mx-auto">
                <img alt="The DX by galargh logo - the letters D and X subtitled with by galargh"
                  src="/images/dx.svg" class="max-w-full
                rounded-lg h-auto" />
              </div>
            </div>
            <div>
              <p class="text-lg font-semibold mb-4">{{ footer_products_heading }}</p>
              <ul class="text-gray-400 space-y-2">
                {% for card in products_cards %}
                  <li>
                    <a href="#products" class="hover:text-white transition duration-200">{{ card.heading }}</a>
                  </li>
                {% endfor %}
              </ul>
            </div>
            <div>
              <p class="text-lg font-semibold mb-4">{{ footer_services_heading }}</p>
              <ul class="text-gray-400 space-y-2">
                {% for card in services_cards %}
                  <li>
                    <a href="#services" class="hover:text-white transition duration-200">{{ card.heading }}</a>
                  </li>
                {% endfor %}
              </ul>
            </div>
            <div>
              <div>
                <p class="text-lg font-semibold mb-4">{{ footer_company_heading }}</p>
                <ul class="text-gray-400 space-y-2">
                  <li>
                    <a href="#about" class="hover:text-white transition duration-200">{{ footer_about_heading }}</a>
                  </li>
                  <li>
                    <a href="{{ calendly_link }}" target="_blank" class="hover:text-white transition duration-200">{{ footer_contact_heading }}</a>
                  </li>
                </ul>
              </div>
              <div class="mt-6">
                <p class="text-lg font-semibold mb-4">{{ footer_connect_heading }}</p>
                <div class="flex space-x-4">
